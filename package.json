{
  "name": "@sentry/fullstory",
  "version": "1.1.5",
  "description": "The Sentry-FullStory integration creates a link from the Sentry Error to the FullStory replay. It also creates a link from the FullStory event to the Sentry error.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && rollup -c",
    "watch": "yarn clean && rollup -cw",
    "lint": "yarn lint:prettier && yarn lint:eslint",
    "lint:prettier": "prettier --check src/**/*.ts",
    "lint:eslint": "eslint src/**/*.ts --cache --format stylish",
    "fix": "yarn fix:eslint && yarn fix:prettier",
    "fix:prettier": "prettier --write src/**/*.ts",
    "fix:eslint": "eslint src/**/*.ts --format stylish --fix"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getsentry/sentry-fullstory.git"
  },
  "author": "Stephen Cefali",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/getsentry/sentry-fullstory/issues"
  },
  "homepage": "https://github.com/getsentry/sentry-fullstory#readme",
  "peerDependencies": {
    "@fullstory/browser": ">=1.0.0",
    "@sentry/browser": ">=4.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@fullstory/browser": "^1.0.1",
    "@sentry-internal/eslint-config-sdk": "^5.21.0",
    "@sentry/browser": "^5.10.2",
    "@sentry/types": "^5.10.0",
    "eslint": "^7.6.0",
    "prettier": "1.19.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.26.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-typescript2": "^0.25.3",
    "typescript": "^3.8"
  }
}
